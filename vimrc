" .vimrc

inoremap jk <Esc>

nnoremap o o<Esc>
nnoremap O O<Esc>

nnoremap <S-l> :bnext<CR>
nnoremap <S-h> :bprevious<CR>

nnoremap :W<CR> :w<CR>
nnoremap :Q<CR> :q<CR>
nnoremap :Wq<CR> :wq<CR>
nnoremap :WQ<CR> :wq<CR>

nnoremap <C-h> <c-w><C-h>
nnoremap <C-j> <c-w><C-j>
nnoremap <C-k> <c-w><C-k>
nnoremap <C-l> <c-w><C-l>
